public class FieldODEState&lt;T extends RealFieldElement&lt;T&gt;&gt; {

    /** Time. */
    private final T time;

    /** Main state at time. */
    private final T[] state;

    /** Secondary state at time. */
    private final T[][] secondaryState;

    /** Simple constructor.
