<a class="jxr_linenumber" name="L575" href="#L575">575</a> <em class="jxr_javadoccomment">     * @throws MathIllegalArgumentException if the underlynig distribution throws one</em>
<a class="jxr_linenumber" name="L576" href="#L576">576</a> <em class="jxr_javadoccomment">     * @since 2.2</em>
<a class="jxr_linenumber" name="L577" href="#L577">577</a> <em class="jxr_javadoccomment">     * @deprecated use the distribution's sample() method</em>
<a class="jxr_linenumber" name="L578" href="#L578">578</a> <em class="jxr_javadoccomment">     */</em>
<a class="jxr_linenumber" name="L579" href="#L579">579</a>     @Deprecated
<a class="jxr_linenumber" name="L580" href="#L580">580</a>     <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">int</strong> nextInversionDeviate(<a href="../../../../../org/apache/commons/math3/distribution/IntegerDistribution.html">IntegerDistribution</a> distribution)
<a class="jxr_linenumber" name="L581" href="#L581">581</a>         <strong class="jxr_keyword">throws</strong> <a href="../../../../../org/apache/commons/math3/exception/MathIllegalArgumentException.html">MathIllegalArgumentException</a> {
<a class="jxr_linenumber" name="L582" href="#L582">582</a>         <strong class="jxr_keyword">return</strong> distribution.inverseCumulativeProbability(nextUniform(0, 1));
