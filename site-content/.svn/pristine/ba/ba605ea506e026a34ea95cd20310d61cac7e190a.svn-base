<span class="sourceLineNo">085</span>     */<a name="line.85"></a>
<span class="sourceLineNo">086</span>    public KolmogorovSmirnovDistribution(int n)<a name="line.86"></a>
<span class="sourceLineNo">087</span>        throws NotStrictlyPositiveException {<a name="line.87"></a>
<span class="sourceLineNo">088</span>        if (n &lt;= 0) {<a name="line.88"></a>
<span class="sourceLineNo">089</span>            throw new NotStrictlyPositiveException(LocalizedFormats.NOT_POSITIVE_NUMBER_OF_SAMPLES, n);<a name="line.89"></a>
<span class="sourceLineNo">090</span>        }<a name="line.90"></a>
