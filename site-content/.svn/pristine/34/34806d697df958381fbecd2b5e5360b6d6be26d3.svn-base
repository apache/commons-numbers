<span class="sourceLineNo">194</span>        long p0 = 1;<a name="line.194"></a>
<span class="sourceLineNo">195</span>        long q0 = 0;<a name="line.195"></a>
<span class="sourceLineNo">196</span>        long p1 = a0;<a name="line.196"></a>
<span class="sourceLineNo">197</span>        long q1 = 1;<a name="line.197"></a>
<span class="sourceLineNo">198</span><a name="line.198"></a>
<span class="sourceLineNo">199</span>        long p2 = 0;<a name="line.199"></a>
<span class="sourceLineNo">200</span>        long q2 = 1;<a name="line.200"></a>
<span class="sourceLineNo">201</span><a name="line.201"></a>
<span class="sourceLineNo">202</span>        int n = 0;<a name="line.202"></a>
<span class="sourceLineNo">203</span>        boolean stop = false;<a name="line.203"></a>
<span class="sourceLineNo">204</span>        do {<a name="line.204"></a>
<span class="sourceLineNo">205</span>            ++n;<a name="line.205"></a>
<span class="sourceLineNo">206</span>            double r1 = 1.0 / (r0 - a0);<a name="line.206"></a>
