<span class="sourceLineNo">178</span>        throws FractionConversionException<a name="line.178"></a>
<span class="sourceLineNo">179</span>    {<a name="line.179"></a>
<span class="sourceLineNo">180</span>        long overflow = Integer.MAX_VALUE;<a name="line.180"></a>
<span class="sourceLineNo">181</span>        double r0 = value;<a name="line.181"></a>
<span class="sourceLineNo">182</span>        long a0 = (long)FastMath.floor(r0);<a name="line.182"></a>
