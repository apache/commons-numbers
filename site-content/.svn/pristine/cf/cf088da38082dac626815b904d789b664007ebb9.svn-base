<a class="jxr_linenumber" name="L93" href="#L93">93</a>              <strong class="jxr_keyword">if</strong> (normalization == DctNormalization.ORTHOGONAL_DCT_I) {
<a class="jxr_linenumber" name="L94" href="#L94">94</a>                  <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">double</strong> s = FastMath.sqrt(2.0 / (f.length - 1));
<a class="jxr_linenumber" name="L95" href="#L95">95</a>                  <strong class="jxr_keyword">return</strong> TransformUtils.scaleArray(fct(f), s);
<a class="jxr_linenumber" name="L96" href="#L96">96</a>              }
