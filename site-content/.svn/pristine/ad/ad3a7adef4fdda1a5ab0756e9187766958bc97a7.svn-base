<span class="sourceLineNo">1348</span>      o1[0] = x10 - 0.5 * (x10 * mx00 + x11 * mx10 + x12 * mx20 - m1[0]);<a name="line.1348"></a>
<span class="sourceLineNo">1349</span>      o1[1] = x11 - 0.5 * (x10 * mx01 + x11 * mx11 + x12 * mx21 - m1[1]);<a name="line.1349"></a>
<span class="sourceLineNo">1350</span>      o1[2] = x12 - 0.5 * (x10 * mx02 + x11 * mx12 + x12 * mx22 - m1[2]);<a name="line.1350"></a>
<span class="sourceLineNo">1351</span>      o2[0] = x20 - 0.5 * (x20 * mx00 + x21 * mx10 + x22 * mx20 - m2[0]);<a name="line.1351"></a>
<span class="sourceLineNo">1352</span>      o2[1] = x21 - 0.5 * (x20 * mx01 + x21 * mx11 + x22 * mx21 - m2[1]);<a name="line.1352"></a>
<span class="sourceLineNo">1353</span>      o2[2] = x22 - 0.5 * (x20 * mx02 + x21 * mx12 + x22 * mx22 - m2[2]);<a name="line.1353"></a>
<span class="sourceLineNo">1354</span><a name="line.1354"></a>
<span class="sourceLineNo">1355</span>      // correction on each elements<a name="line.1355"></a>
<span class="sourceLineNo">1356</span>      double corr00 = o0[0] - m0[0];<a name="line.1356"></a>
<span class="sourceLineNo">1357</span>      double corr01 = o0[1] - m0[1];<a name="line.1357"></a>
<span class="sourceLineNo">1358</span>      double corr02 = o0[2] - m0[2];<a name="line.1358"></a>
<span class="sourceLineNo">1359</span>      double corr10 = o1[0] - m1[0];<a name="line.1359"></a>
<span class="sourceLineNo">1360</span>      double corr11 = o1[1] - m1[1];<a name="line.1360"></a>
<span class="sourceLineNo">1361</span>      double corr12 = o1[2] - m1[2];<a name="line.1361"></a>
<span class="sourceLineNo">1362</span>      double corr20 = o2[0] - m2[0];<a name="line.1362"></a>
<span class="sourceLineNo">1363</span>      double corr21 = o2[1] - m2[1];<a name="line.1363"></a>
<span class="sourceLineNo">1364</span>      double corr22 = o2[2] - m2[2];<a name="line.1364"></a>
<span class="sourceLineNo">1365</span><a name="line.1365"></a>
