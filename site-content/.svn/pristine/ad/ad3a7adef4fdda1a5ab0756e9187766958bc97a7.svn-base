<span class="sourceLineNo">1319</span>    double x21 = m2[1];<a name="line.1319"></a>
<span class="sourceLineNo">1320</span>    double x22 = m2[2];<a name="line.1320"></a>
<span class="sourceLineNo">1321</span>    double fn = 0;<a name="line.1321"></a>
<span class="sourceLineNo">1322</span>    double fn1;<a name="line.1322"></a>
<span class="sourceLineNo">1323</span><a name="line.1323"></a>
<span class="sourceLineNo">1324</span>    double[][] o = new double[3][3];<a name="line.1324"></a>
<span class="sourceLineNo">1325</span>    double[] o0 = o[0];<a name="line.1325"></a>
<span class="sourceLineNo">1326</span>    double[] o1 = o[1];<a name="line.1326"></a>
<span class="sourceLineNo">1327</span>    double[] o2 = o[2];<a name="line.1327"></a>
<span class="sourceLineNo">1328</span><a name="line.1328"></a>
<span class="sourceLineNo">1329</span>    // iterative correction: Xn+1 = Xn - 0.5 * (Xn.Mt.Xn - M)<a name="line.1329"></a>
<span class="sourceLineNo">1330</span>    int i = 0;<a name="line.1330"></a>
<span class="sourceLineNo">1331</span>    while (++i &lt; 11) {<a name="line.1331"></a>
<span class="sourceLineNo">1332</span><a name="line.1332"></a>
<span class="sourceLineNo">1333</span>      // Mt.Xn<a name="line.1333"></a>
<span class="sourceLineNo">1334</span>      double mx00 = m0[0] * x00 + m1[0] * x10 + m2[0] * x20;<a name="line.1334"></a>
<span class="sourceLineNo">1335</span>      double mx10 = m0[1] * x00 + m1[1] * x10 + m2[1] * x20;<a name="line.1335"></a>
<span class="sourceLineNo">1336</span>      double mx20 = m0[2] * x00 + m1[2] * x10 + m2[2] * x20;<a name="line.1336"></a>
<span class="sourceLineNo">1337</span>      double mx01 = m0[0] * x01 + m1[0] * x11 + m2[0] * x21;<a name="line.1337"></a>
<span class="sourceLineNo">1338</span>      double mx11 = m0[1] * x01 + m1[1] * x11 + m2[1] * x21;<a name="line.1338"></a>
<span class="sourceLineNo">1339</span>      double mx21 = m0[2] * x01 + m1[2] * x11 + m2[2] * x21;<a name="line.1339"></a>
<span class="sourceLineNo">1340</span>      double mx02 = m0[0] * x02 + m1[0] * x12 + m2[0] * x22;<a name="line.1340"></a>
<span class="sourceLineNo">1341</span>      double mx12 = m0[1] * x02 + m1[1] * x12 + m2[1] * x22;<a name="line.1341"></a>
<span class="sourceLineNo">1342</span>      double mx22 = m0[2] * x02 + m1[2] * x12 + m2[2] * x22;<a name="line.1342"></a>
