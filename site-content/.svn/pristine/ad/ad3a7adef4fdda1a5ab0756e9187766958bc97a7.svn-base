<span class="sourceLineNo">1289</span>  private Rotation composeInverseInternal(Rotation r) {<a name="line.1289"></a>
<span class="sourceLineNo">1290</span>    return new Rotation(-r.q0 * q0 - (r.q1 * q1 + r.q2 * q2 + r.q3 * q3),<a name="line.1290"></a>
