<span class="sourceLineNo">1367</span>      fn1 = corr00 * corr00 + corr01 * corr01 + corr02 * corr02 +<a name="line.1367"></a>
<span class="sourceLineNo">1368</span>            corr10 * corr10 + corr11 * corr11 + corr12 * corr12 +<a name="line.1368"></a>
<span class="sourceLineNo">1369</span>            corr20 * corr20 + corr21 * corr21 + corr22 * corr22;<a name="line.1369"></a>
<span class="sourceLineNo">1370</span><a name="line.1370"></a>
<span class="sourceLineNo">1371</span>      // convergence test<a name="line.1371"></a>
<span class="sourceLineNo">1372</span>      if (FastMath.abs(fn1 - fn) &lt;= threshold) {<a name="line.1372"></a>
<span class="sourceLineNo">1373</span>          return o;<a name="line.1373"></a>
<span class="sourceLineNo">1374</span>      }<a name="line.1374"></a>
<span class="sourceLineNo">1375</span><a name="line.1375"></a>
<span class="sourceLineNo">1376</span>      // prepare next iteration<a name="line.1376"></a>
<span class="sourceLineNo">1377</span>      x00 = o0[0];<a name="line.1377"></a>
<span class="sourceLineNo">1378</span>      x01 = o0[1];<a name="line.1378"></a>
<span class="sourceLineNo">1379</span>      x02 = o0[2];<a name="line.1379"></a>
<span class="sourceLineNo">1380</span>      x10 = o1[0];<a name="line.1380"></a>
<span class="sourceLineNo">1381</span>      x11 = o1[1];<a name="line.1381"></a>
<span class="sourceLineNo">1382</span>      x12 = o1[2];<a name="line.1382"></a>
<span class="sourceLineNo">1383</span>      x20 = o2[0];<a name="line.1383"></a>
<span class="sourceLineNo">1384</span>      x21 = o2[1];<a name="line.1384"></a>
<span class="sourceLineNo">1385</span>      x22 = o2[2];<a name="line.1385"></a>
<span class="sourceLineNo">1386</span>      fn  = fn1;<a name="line.1386"></a>
<span class="sourceLineNo">1387</span><a name="line.1387"></a>
<span class="sourceLineNo">1388</span>    }<a name="line.1388"></a>
<span class="sourceLineNo">1389</span><a name="line.1389"></a>
<span class="sourceLineNo">1390</span>    // the algorithm did not converge after 10 iterations<a name="line.1390"></a>
<span class="sourceLineNo">1391</span>    throw new NotARotationMatrixException(<a name="line.1391"></a>
<span class="sourceLineNo">1392</span>            LocalizedFormats.UNABLE_TO_ORTHOGONOLIZE_MATRIX,<a name="line.1392"></a>
<span class="sourceLineNo">1393</span>            i - 1);<a name="line.1393"></a>
<span class="sourceLineNo">1394</span>  }<a name="line.1394"></a>
