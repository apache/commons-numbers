<span class="sourceLineNo">1308</span>    throws NotARotationMatrixException {<a name="line.1308"></a>
<span class="sourceLineNo">1309</span>    double[] m0 = m[0];<a name="line.1309"></a>
<span class="sourceLineNo">1310</span>    double[] m1 = m[1];<a name="line.1310"></a>
<span class="sourceLineNo">1311</span>    double[] m2 = m[2];<a name="line.1311"></a>
<span class="sourceLineNo">1312</span>    double x00 = m0[0];<a name="line.1312"></a>
<span class="sourceLineNo">1313</span>    double x01 = m0[1];<a name="line.1313"></a>
<span class="sourceLineNo">1314</span>    double x02 = m0[2];<a name="line.1314"></a>
<span class="sourceLineNo">1315</span>    double x10 = m1[0];<a name="line.1315"></a>
<span class="sourceLineNo">1316</span>    double x11 = m1[1];<a name="line.1316"></a>
