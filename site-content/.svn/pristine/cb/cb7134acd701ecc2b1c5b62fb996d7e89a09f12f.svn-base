<a class="jxr_linenumber" name="L160" href="#L160">160</a>     @Test
<a class="jxr_linenumber" name="L161" href="#L161">161</a>     <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">void</strong> testSolutionLeftSide() {
<a class="jxr_linenumber" name="L162" href="#L162">162</a>         UnivariateFunction f = <strong class="jxr_keyword">new</strong> Sin();
