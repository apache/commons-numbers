<a class="jxr_linenumber" name="L136" href="#L136">136</a>     @Test
<a class="jxr_linenumber" name="L137" href="#L137">137</a>     <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">void</strong> testBadEndpoints() {
<a class="jxr_linenumber" name="L138" href="#L138">138</a>         UnivariateFunction f = <strong class="jxr_keyword">new</strong> Sin();
