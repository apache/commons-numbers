<a class="jxr_linenumber" name="L134" href="#L134">134</a>         <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">double</strong> cosT = FastMath.cos(t);
<a class="jxr_linenumber" name="L135" href="#L135">135</a>         <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">double</strong> sinT = FastMath.sin(t);
<a class="jxr_linenumber" name="L136" href="#L136">136</a>         omegaReal = <strong class="jxr_keyword">new</strong> <strong class="jxr_keyword">double</strong>[absN];
<a class="jxr_linenumber" name="L137" href="#L137">137</a>         omegaImaginaryCounterClockwise = <strong class="jxr_keyword">new</strong> <strong class="jxr_keyword">double</strong>[absN];
<a class="jxr_linenumber" name="L138" href="#L138">138</a>         omegaImaginaryClockwise = <strong class="jxr_keyword">new</strong> <strong class="jxr_keyword">double</strong>[absN];
<a class="jxr_linenumber" name="L139" href="#L139">139</a>         omegaReal[0] = 1.0;
<a class="jxr_linenumber" name="L140" href="#L140">140</a>         omegaImaginaryCounterClockwise[0] = 0.0;
<a class="jxr_linenumber" name="L141" href="#L141">141</a>         omegaImaginaryClockwise[0] = 0.0;
<a class="jxr_linenumber" name="L142" href="#L142">142</a>         <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">int</strong> i = 1; i &lt; absN; i++) {
<a class="jxr_linenumber" name="L143" href="#L143">143</a>             omegaReal[i] = omegaReal[i - 1] * cosT -
