<a class="jxr_linenumber" name="L1294" href="#L1294">1294</a>         <em class="jxr_comment">// test z_2</em>
<a class="jxr_linenumber" name="L1295" href="#L1295">1295</a>         Assert.assertEquals(-1.0,               thirdRootsOfZ[2].getReal(),      1.0e-5);
<a class="jxr_linenumber" name="L1296" href="#L1296">1296</a>         Assert.assertEquals(-1.732050807568877, thirdRootsOfZ[2].getImaginary(), 1.0e-5);
