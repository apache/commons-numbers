<a class="jxr_linenumber" name="L1202" href="#L1202">1202</a> 
<a class="jxr_linenumber" name="L1203" href="#L1203">1203</a>     <em class="jxr_javadoccomment">/**</em>
<a class="jxr_linenumber" name="L1204" href="#L1204">1204</a> <em class="jxr_javadoccomment">     * Test: computing &lt;b&gt;third roots&lt;/b&gt; of z.</em>
<a class="jxr_linenumber" name="L1205" href="#L1205">1205</a> <em class="jxr_javadoccomment">     * &lt;pre&gt;</em>
<a class="jxr_linenumber" name="L1206" href="#L1206">1206</a> <em class="jxr_javadoccomment">     * &lt;code&gt;</em>
<a class="jxr_linenumber" name="L1207" href="#L1207">1207</a> <em class="jxr_javadoccomment">     * &lt;b&gt;z = -2 + 2 * i&lt;/b&gt;</em>
<a class="jxr_linenumber" name="L1208" href="#L1208">1208</a> <em class="jxr_javadoccomment">     *   =&gt; z_0 =  1      +          i</em>
<a class="jxr_linenumber" name="L1209" href="#L1209">1209</a> <em class="jxr_javadoccomment">     *   =&gt; z_1 = -1.3660 + 0.3660 * i</em>
<a class="jxr_linenumber" name="L1210" href="#L1210">1210</a> <em class="jxr_javadoccomment">     *   =&gt; z_2 =  0.3660 - 1.3660 * i</em>
<a class="jxr_linenumber" name="L1211" href="#L1211">1211</a> <em class="jxr_javadoccomment">     * &lt;/code&gt;</em>
<a class="jxr_linenumber" name="L1212" href="#L1212">1212</a> <em class="jxr_javadoccomment">     * &lt;/pre&gt;</em>
<a class="jxr_linenumber" name="L1213" href="#L1213">1213</a> <em class="jxr_javadoccomment">     */</em>
<a class="jxr_linenumber" name="L1214" href="#L1214">1214</a>     @Test
<a class="jxr_linenumber" name="L1215" href="#L1215">1215</a>     <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">void</strong> testNthRoot_normal_thirdRoot() {
<a class="jxr_linenumber" name="L1216" href="#L1216">1216</a>         <em class="jxr_comment">// The complex number we want to compute all third-roots for.</em>
<a class="jxr_linenumber" name="L1217" href="#L1217">1217</a>         Complex z = <strong class="jxr_keyword">new</strong> Complex(-2,2);
<a class="jxr_linenumber" name="L1218" href="#L1218">1218</a>         <em class="jxr_comment">// The List holding all third roots</em>
<a class="jxr_linenumber" name="L1219" href="#L1219">1219</a>         Complex[] thirdRootsOfZ = z.nthRoot(3).toArray(<strong class="jxr_keyword">new</strong> Complex[0]);
<a class="jxr_linenumber" name="L1220" href="#L1220">1220</a>         <em class="jxr_comment">// Returned Collection must not be empty!</em>
<a class="jxr_linenumber" name="L1221" href="#L1221">1221</a>         Assert.assertEquals(3, thirdRootsOfZ.length);
<a class="jxr_linenumber" name="L1222" href="#L1222">1222</a>         <em class="jxr_comment">// test z_0</em>
<a class="jxr_linenumber" name="L1223" href="#L1223">1223</a>         Assert.assertEquals(1.0,                  thirdRootsOfZ[0].getReal(),      1.0e-5);
<a class="jxr_linenumber" name="L1224" href="#L1224">1224</a>         Assert.assertEquals(1.0,                  thirdRootsOfZ[0].getImaginary(), 1.0e-5);
<a class="jxr_linenumber" name="L1225" href="#L1225">1225</a>         <em class="jxr_comment">// test z_1</em>
<a class="jxr_linenumber" name="L1226" href="#L1226">1226</a>         Assert.assertEquals(-1.3660254037844386,  thirdRootsOfZ[1].getReal(),      1.0e-5);
<a class="jxr_linenumber" name="L1227" href="#L1227">1227</a>         Assert.assertEquals(0.36602540378443843,  thirdRootsOfZ[1].getImaginary(), 1.0e-5);
<a class="jxr_linenumber" name="L1228" href="#L1228">1228</a>         <em class="jxr_comment">// test z_2</em>
<a class="jxr_linenumber" name="L1229" href="#L1229">1229</a>         Assert.assertEquals(0.366025403784439,    thirdRootsOfZ[2].getReal(),      1.0e-5);
<a class="jxr_linenumber" name="L1230" href="#L1230">1230</a>         Assert.assertEquals(-1.3660254037844384,  thirdRootsOfZ[2].getImaginary(), 1.0e-5);
<a class="jxr_linenumber" name="L1231" href="#L1231">1231</a>     }
<a class="jxr_linenumber" name="L1232" href="#L1232">1232</a> 
<a class="jxr_linenumber" name="L1233" href="#L1233">1233</a> 
<a class="jxr_linenumber" name="L1234" href="#L1234">1234</a>     <em class="jxr_javadoccomment">/**</em>
<a class="jxr_linenumber" name="L1235" href="#L1235">1235</a> <em class="jxr_javadoccomment">     * Test: computing &lt;b&gt;fourth roots&lt;/b&gt; of z.</em>
<a class="jxr_linenumber" name="L1236" href="#L1236">1236</a> <em class="jxr_javadoccomment">     * &lt;pre&gt;</em>
<a class="jxr_linenumber" name="L1237" href="#L1237">1237</a> <em class="jxr_javadoccomment">     * &lt;code&gt;</em>
<a class="jxr_linenumber" name="L1238" href="#L1238">1238</a> <em class="jxr_javadoccomment">     * &lt;b&gt;z = 5 - 2 * i&lt;/b&gt;</em>
<a class="jxr_linenumber" name="L1239" href="#L1239">1239</a> <em class="jxr_javadoccomment">     *   =&gt; z_0 =  1.5164 - 0.1446 * i</em>
<a class="jxr_linenumber" name="L1240" href="#L1240">1240</a> <em class="jxr_javadoccomment">     *   =&gt; z_1 =  0.1446 + 1.5164 * i</em>
<a class="jxr_linenumber" name="L1241" href="#L1241">1241</a> <em class="jxr_javadoccomment">     *   =&gt; z_2 = -1.5164 + 0.1446 * i</em>
<a class="jxr_linenumber" name="L1242" href="#L1242">1242</a> <em class="jxr_javadoccomment">     *   =&gt; z_3 = -1.5164 - 0.1446 * i</em>
<a class="jxr_linenumber" name="L1243" href="#L1243">1243</a> <em class="jxr_javadoccomment">     * &lt;/code&gt;</em>
