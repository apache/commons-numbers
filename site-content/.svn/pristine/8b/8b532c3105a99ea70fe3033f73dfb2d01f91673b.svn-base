<a class="jxr_linenumber" name="L36" href="#L36">36</a>      @Test
<a class="jxr_linenumber" name="L37" href="#L37">37</a>      <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">void</strong> testLinearFunction() {
<a class="jxr_linenumber" name="L38" href="#L38">38</a>          PolynomialFunctionNewtonForm p;
<a class="jxr_linenumber" name="L39" href="#L39">39</a>          <strong class="jxr_keyword">double</strong> coefficients[], z, expected, result, tolerance = 1E-12;
<a class="jxr_linenumber" name="L40" href="#L40">40</a>  
<a class="jxr_linenumber" name="L41" href="#L41">41</a>          <em class="jxr_comment">// p(x) = 1.5x - 4 = 2 + 1.5(x-4)</em>
<a class="jxr_linenumber" name="L42" href="#L42">42</a>          <strong class="jxr_keyword">double</strong> a[] = { 2.0, 1.5 };
<a class="jxr_linenumber" name="L43" href="#L43">43</a>          <strong class="jxr_keyword">double</strong> c[] = { 4.0 };
<a class="jxr_linenumber" name="L44" href="#L44">44</a>          p = <strong class="jxr_keyword">new</strong> PolynomialFunctionNewtonForm(a, c);
<a class="jxr_linenumber" name="L45" href="#L45">45</a>  
<a class="jxr_linenumber" name="L46" href="#L46">46</a>          z = 2.0; expected = -1.0; result = p.value(z);
<a class="jxr_linenumber" name="L47" href="#L47">47</a>          Assert.assertEquals(expected, result, tolerance);
<a class="jxr_linenumber" name="L48" href="#L48">48</a>  
<a class="jxr_linenumber" name="L49" href="#L49">49</a>          z = 4.5; expected = 2.75; result = p.value(z);
<a class="jxr_linenumber" name="L50" href="#L50">50</a>          Assert.assertEquals(expected, result, tolerance);
<a class="jxr_linenumber" name="L51" href="#L51">51</a>  
<a class="jxr_linenumber" name="L52" href="#L52">52</a>          z = 6.0; expected = 5.0; result = p.value(z);
<a class="jxr_linenumber" name="L53" href="#L53">53</a>          Assert.assertEquals(expected, result, tolerance);
<a class="jxr_linenumber" name="L54" href="#L54">54</a>  
<a class="jxr_linenumber" name="L55" href="#L55">55</a>          Assert.assertEquals(1, p.degree());
<a class="jxr_linenumber" name="L56" href="#L56">56</a>  
<a class="jxr_linenumber" name="L57" href="#L57">57</a>          coefficients = p.getCoefficients();
<a class="jxr_linenumber" name="L58" href="#L58">58</a>          Assert.assertEquals(2, coefficients.length);
<a class="jxr_linenumber" name="L59" href="#L59">59</a>          Assert.assertEquals(-4.0, coefficients[0], tolerance);
<a class="jxr_linenumber" name="L60" href="#L60">60</a>          Assert.assertEquals(1.5, coefficients[1], tolerance);
<a class="jxr_linenumber" name="L61" href="#L61">61</a>      }
