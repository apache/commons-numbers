<a class="jxr_linenumber" name="L63" href="#L63">63</a>      <em class="jxr_javadoccomment">/**</em>
<a class="jxr_linenumber" name="L64" href="#L64">64</a>  <em class="jxr_javadoccomment">     * Test of polynomial for the quadratic function.</em>
<a class="jxr_linenumber" name="L65" href="#L65">65</a>  <em class="jxr_javadoccomment">     */</em>
<a class="jxr_linenumber" name="L66" href="#L66">66</a>      @Test
<a class="jxr_linenumber" name="L67" href="#L67">67</a>      <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">void</strong> testQuadraticFunction() {
<a class="jxr_linenumber" name="L68" href="#L68">68</a>          PolynomialFunctionNewtonForm p;
<a class="jxr_linenumber" name="L69" href="#L69">69</a>          <strong class="jxr_keyword">double</strong> coefficients[], z, expected, result, tolerance = 1E-12;
<a class="jxr_linenumber" name="L70" href="#L70">70</a>  
<a class="jxr_linenumber" name="L71" href="#L71">71</a>          <em class="jxr_comment">// p(x) = 2x^2 + 5x - 3 = 4 + 3(x-1) + 2(x-1)(x+2)</em>
<a class="jxr_linenumber" name="L72" href="#L72">72</a>          <strong class="jxr_keyword">double</strong> a[] = { 4.0, 3.0, 2.0 };
<a class="jxr_linenumber" name="L73" href="#L73">73</a>          <strong class="jxr_keyword">double</strong> c[] = { 1.0, -2.0 };
<a class="jxr_linenumber" name="L74" href="#L74">74</a>          p = <strong class="jxr_keyword">new</strong> PolynomialFunctionNewtonForm(a, c);
<a class="jxr_linenumber" name="L75" href="#L75">75</a>  
<a class="jxr_linenumber" name="L76" href="#L76">76</a>          z = 1.0; expected = 4.0; result = p.value(z);
<a class="jxr_linenumber" name="L77" href="#L77">77</a>          Assert.assertEquals(expected, result, tolerance);
<a class="jxr_linenumber" name="L78" href="#L78">78</a>  
<a class="jxr_linenumber" name="L79" href="#L79">79</a>          z = 2.5; expected = 22.0; result = p.value(z);
<a class="jxr_linenumber" name="L80" href="#L80">80</a>          Assert.assertEquals(expected, result, tolerance);
<a class="jxr_linenumber" name="L81" href="#L81">81</a>  
<a class="jxr_linenumber" name="L82" href="#L82">82</a>          z = -2.0; expected = -5.0; result = p.value(z);
<a class="jxr_linenumber" name="L83" href="#L83">83</a>          Assert.assertEquals(expected, result, tolerance);
<a class="jxr_linenumber" name="L84" href="#L84">84</a>  
<a class="jxr_linenumber" name="L85" href="#L85">85</a>          Assert.assertEquals(2, p.degree());
<a class="jxr_linenumber" name="L86" href="#L86">86</a>  
<a class="jxr_linenumber" name="L87" href="#L87">87</a>          coefficients = p.getCoefficients();
<a class="jxr_linenumber" name="L88" href="#L88">88</a>          Assert.assertEquals(3, coefficients.length);
<a class="jxr_linenumber" name="L89" href="#L89">89</a>          Assert.assertEquals(-3.0, coefficients[0], tolerance);
<a class="jxr_linenumber" name="L90" href="#L90">90</a>          Assert.assertEquals(5.0, coefficients[1], tolerance);
<a class="jxr_linenumber" name="L91" href="#L91">91</a>          Assert.assertEquals(2.0, coefficients[2], tolerance);
<a class="jxr_linenumber" name="L92" href="#L92">92</a>      }
<a class="jxr_linenumber" name="L93" href="#L93">93</a>  
<a class="jxr_linenumber" name="L94" href="#L94">94</a>      <em class="jxr_javadoccomment">/**</em>
<a class="jxr_linenumber" name="L95" href="#L95">95</a>  <em class="jxr_javadoccomment">     * Test of polynomial for the quintic function.</em>
<a class="jxr_linenumber" name="L96" href="#L96">96</a>  <em class="jxr_javadoccomment">     */</em>
<a class="jxr_linenumber" name="L97" href="#L97">97</a>      @Test
<a class="jxr_linenumber" name="L98" href="#L98">98</a>      <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">void</strong> testQuinticFunction() {
<a class="jxr_linenumber" name="L99" href="#L99">99</a>          PolynomialFunctionNewtonForm p;
<a class="jxr_linenumber" name="L100" href="#L100">100</a>         <strong class="jxr_keyword">double</strong> coefficients[], z, expected, result, tolerance = 1E-12;
<a class="jxr_linenumber" name="L101" href="#L101">101</a> 
<a class="jxr_linenumber" name="L102" href="#L102">102</a>         <em class="jxr_comment">// p(x) = x^5 - x^4 - 7x^3 + x^2 + 6x</em>
<a class="jxr_linenumber" name="L103" href="#L103">103</a>         <em class="jxr_comment">//      = 6x - 6x^2 -6x^2(x-1) + x^2(x-1)(x+1) + x^2(x-1)(x+1)(x-2)</em>
<a class="jxr_linenumber" name="L104" href="#L104">104</a>         <strong class="jxr_keyword">double</strong> a[] = { 0.0, 6.0, -6.0, -6.0, 1.0, 1.0 };
<a class="jxr_linenumber" name="L105" href="#L105">105</a>         <strong class="jxr_keyword">double</strong> c[] = { 0.0, 0.0, 1.0, -1.0, 2.0 };
<a class="jxr_linenumber" name="L106" href="#L106">106</a>         p = <strong class="jxr_keyword">new</strong> PolynomialFunctionNewtonForm(a, c);
<a class="jxr_linenumber" name="L107" href="#L107">107</a> 
<a class="jxr_linenumber" name="L108" href="#L108">108</a>         z = 0.0; expected = 0.0; result = p.value(z);
<a class="jxr_linenumber" name="L109" href="#L109">109</a>         Assert.assertEquals(expected, result, tolerance);
<a class="jxr_linenumber" name="L110" href="#L110">110</a> 
<a class="jxr_linenumber" name="L111" href="#L111">111</a>         z = -2.0; expected = 0.0; result = p.value(z);
<a class="jxr_linenumber" name="L112" href="#L112">112</a>         Assert.assertEquals(expected, result, tolerance);
<a class="jxr_linenumber" name="L113" href="#L113">113</a> 
<a class="jxr_linenumber" name="L114" href="#L114">114</a>         z = 4.0; expected = 360.0; result = p.value(z);
<a class="jxr_linenumber" name="L115" href="#L115">115</a>         Assert.assertEquals(expected, result, tolerance);
<a class="jxr_linenumber" name="L116" href="#L116">116</a> 
<a class="jxr_linenumber" name="L117" href="#L117">117</a>         Assert.assertEquals(5, p.degree());
<a class="jxr_linenumber" name="L118" href="#L118">118</a> 
<a class="jxr_linenumber" name="L119" href="#L119">119</a>         coefficients = p.getCoefficients();
<a class="jxr_linenumber" name="L120" href="#L120">120</a>         Assert.assertEquals(6, coefficients.length);
<a class="jxr_linenumber" name="L121" href="#L121">121</a>         Assert.assertEquals(0.0, coefficients[0], tolerance);
<a class="jxr_linenumber" name="L122" href="#L122">122</a>         Assert.assertEquals(6.0, coefficients[1], tolerance);
<a class="jxr_linenumber" name="L123" href="#L123">123</a>         Assert.assertEquals(1.0, coefficients[2], tolerance);
<a class="jxr_linenumber" name="L124" href="#L124">124</a>         Assert.assertEquals(-7.0, coefficients[3], tolerance);
<a class="jxr_linenumber" name="L125" href="#L125">125</a>         Assert.assertEquals(-1.0, coefficients[4], tolerance);
<a class="jxr_linenumber" name="L126" href="#L126">126</a>         Assert.assertEquals(1.0, coefficients[5], tolerance);
<a class="jxr_linenumber" name="L127" href="#L127">127</a>     }
<a class="jxr_linenumber" name="L128" href="#L128">128</a> 
<a class="jxr_linenumber" name="L129" href="#L129">129</a>     <em class="jxr_javadoccomment">/**</em>
<a class="jxr_linenumber" name="L130" href="#L130">130</a> <em class="jxr_javadoccomment">     * Test for derivatives.</em>
<a class="jxr_linenumber" name="L131" href="#L131">131</a> <em class="jxr_javadoccomment">     */</em>
<a class="jxr_linenumber" name="L132" href="#L132">132</a>     @Test
<a class="jxr_linenumber" name="L133" href="#L133">133</a>     <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">void</strong> testDerivative() {
<a class="jxr_linenumber" name="L134" href="#L134">134</a> 
<a class="jxr_linenumber" name="L135" href="#L135">135</a>         <em class="jxr_comment">// x^3 = 0 * [1] + 1 * [x] + 3 * [x(x-1)] + 1 * [x(x-1)(x-2)]</em>
<a class="jxr_linenumber" name="L136" href="#L136">136</a>         PolynomialFunctionNewtonForm p =
<a class="jxr_linenumber" name="L137" href="#L137">137</a>                 <strong class="jxr_keyword">new</strong> PolynomialFunctionNewtonForm(<strong class="jxr_keyword">new</strong> <strong class="jxr_keyword">double</strong>[] { 0, 1, 3, 1 },
<a class="jxr_linenumber" name="L138" href="#L138">138</a>                                                  <strong class="jxr_keyword">new</strong> <strong class="jxr_keyword">double</strong>[] { 0, 1, 2 });
<a class="jxr_linenumber" name="L139" href="#L139">139</a> 
<a class="jxr_linenumber" name="L140" href="#L140">140</a>         <strong class="jxr_keyword">double</strong> eps = 2.0e-14;
<a class="jxr_linenumber" name="L141" href="#L141">141</a>         <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">double</strong> t = 0.0; t &lt; 10.0; t += 0.1) {
<a class="jxr_linenumber" name="L142" href="#L142">142</a>             DerivativeStructure x = <strong class="jxr_keyword">new</strong> DerivativeStructure(1, 4, 0, t);
<a class="jxr_linenumber" name="L143" href="#L143">143</a>             DerivativeStructure y = p.value(x);
<a class="jxr_linenumber" name="L144" href="#L144">144</a>             Assert.assertEquals(t * t * t,   y.getValue(),              eps * t * t * t);
<a class="jxr_linenumber" name="L145" href="#L145">145</a>             Assert.assertEquals(3.0 * t * t, y.getPartialDerivative(1), eps * 3.0 * t * t);
<a class="jxr_linenumber" name="L146" href="#L146">146</a>             Assert.assertEquals(6.0 * t,     y.getPartialDerivative(2), eps * 6.0 * t);
<a class="jxr_linenumber" name="L147" href="#L147">147</a>             Assert.assertEquals(6.0,         y.getPartialDerivative(3), eps * 6.0);
<a class="jxr_linenumber" name="L148" href="#L148">148</a>             Assert.assertEquals(0.0,         y.getPartialDerivative(4), eps);
<a class="jxr_linenumber" name="L149" href="#L149">149</a>         }
<a class="jxr_linenumber" name="L150" href="#L150">150</a> 
<a class="jxr_linenumber" name="L151" href="#L151">151</a>     }
