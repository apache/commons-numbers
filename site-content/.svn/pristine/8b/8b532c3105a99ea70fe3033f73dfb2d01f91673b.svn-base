<a class="jxr_linenumber" name="L153" href="#L153">153</a>     <em class="jxr_javadoccomment">/**</em>
<a class="jxr_linenumber" name="L154" href="#L154">154</a> <em class="jxr_javadoccomment">     * Test of parameters for the polynomial.</em>
<a class="jxr_linenumber" name="L155" href="#L155">155</a> <em class="jxr_javadoccomment">     */</em>
<a class="jxr_linenumber" name="L156" href="#L156">156</a>     @Test
<a class="jxr_linenumber" name="L157" href="#L157">157</a>     <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">void</strong> testParameters() {
<a class="jxr_linenumber" name="L158" href="#L158">158</a> 
<a class="jxr_linenumber" name="L159" href="#L159">159</a>         <strong class="jxr_keyword">try</strong> {
<a class="jxr_linenumber" name="L160" href="#L160">160</a>             <em class="jxr_comment">// bad input array length</em>
<a class="jxr_linenumber" name="L161" href="#L161">161</a>             <strong class="jxr_keyword">double</strong> a[] = { 1.0 };
<a class="jxr_linenumber" name="L162" href="#L162">162</a>             <strong class="jxr_keyword">double</strong> c[] = { 2.0 };
<a class="jxr_linenumber" name="L163" href="#L163">163</a>             <strong class="jxr_keyword">new</strong> PolynomialFunctionNewtonForm(a, c);
<a class="jxr_linenumber" name="L164" href="#L164">164</a>             Assert.fail(<span class="jxr_string">"Expecting MathIllegalArgumentException - bad input array length"</span>);
<a class="jxr_linenumber" name="L165" href="#L165">165</a>         } <strong class="jxr_keyword">catch</strong> (MathIllegalArgumentException ex) {
<a class="jxr_linenumber" name="L166" href="#L166">166</a>             <em class="jxr_comment">// expected</em>
<a class="jxr_linenumber" name="L167" href="#L167">167</a>         }
<a class="jxr_linenumber" name="L168" href="#L168">168</a>         <strong class="jxr_keyword">try</strong> {
<a class="jxr_linenumber" name="L169" href="#L169">169</a>             <em class="jxr_comment">// mismatch input arrays</em>
<a class="jxr_linenumber" name="L170" href="#L170">170</a>             <strong class="jxr_keyword">double</strong> a[] = { 1.0, 2.0, 3.0, 4.0 };
