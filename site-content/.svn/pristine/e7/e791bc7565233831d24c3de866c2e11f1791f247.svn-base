<span class="fc" id="L628">            yk = one.subtract(s).divide(one.add(s));</span>

<span class="fc" id="L630">            two2kp3 = two2kp3.multiply(four);</span>

<span class="fc" id="L632">            final Dfp p = one.add(yk);</span>
<span class="fc" id="L633">            final Dfp p2 = p.multiply(p);</span>
<span class="fc" id="L634">            ak = ak.multiply(p2.multiply(p2)).subtract(two2kp3.multiply(yk).multiply(one.add(yk).add(yk.multiply(yk))));</span>

