<span class="sourceLineNo">275</span>     */<a name="line.275"></a>
<span class="sourceLineNo">276</span>    BicubicFunction(double[] coeff) {<a name="line.276"></a>
<span class="sourceLineNo">277</span>        a = new double[N][N];<a name="line.277"></a>
<span class="sourceLineNo">278</span>        for (int j = 0; j &lt; N; j++) {<a name="line.278"></a>
<span class="sourceLineNo">279</span>            final double[] aJ = a[j];<a name="line.279"></a>
<span class="sourceLineNo">280</span>            for (int i = 0; i &lt; N; i++) {<a name="line.280"></a>
<span class="sourceLineNo">281</span>                aJ[i] = coeff[i * N + j];<a name="line.281"></a>
<span class="sourceLineNo">282</span>            }<a name="line.282"></a>
<span class="sourceLineNo">283</span>        }<a name="line.283"></a>
