<span class="sourceLineNo">315</span>     */<a name="line.315"></a>
<span class="sourceLineNo">316</span>    private double apply(double[] pX, double[] pY, double[][] coeff) {<a name="line.316"></a>
<span class="sourceLineNo">317</span>        double result = 0;<a name="line.317"></a>
