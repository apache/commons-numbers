<span class="sourceLineNo">300</span><a name="line.300"></a>
<span class="sourceLineNo">301</span>        final double y2 = y * y;<a name="line.301"></a>
<span class="sourceLineNo">302</span>        final double y3 = y2 * y;<a name="line.302"></a>
<span class="sourceLineNo">303</span>        final double[] pY = {1, y, y2, y3};<a name="line.303"></a>
<span class="sourceLineNo">304</span><a name="line.304"></a>
