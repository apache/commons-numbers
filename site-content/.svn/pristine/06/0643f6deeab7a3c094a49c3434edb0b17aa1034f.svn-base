<span class="sourceLineNo">262</span>/**<a name="line.262"></a>
<span class="sourceLineNo">263</span> * Bicubic function.<a name="line.263"></a>
<span class="sourceLineNo">264</span> */<a name="line.264"></a>
<span class="sourceLineNo">265</span>class BicubicFunction implements BivariateFunction {<a name="line.265"></a>
<span class="sourceLineNo">266</span>    /** Number of points. */<a name="line.266"></a>
<span class="sourceLineNo">267</span>    private static final short N = 4;<a name="line.267"></a>
<span class="sourceLineNo">268</span>    /** Coefficients */<a name="line.268"></a>
<span class="sourceLineNo">269</span>    private final double[][] a;<a name="line.269"></a>
<span class="sourceLineNo">270</span><a name="line.270"></a>
<span class="sourceLineNo">271</span>    /**<a name="line.271"></a>
<span class="sourceLineNo">272</span>     * Simple constructor.<a name="line.272"></a>
<span class="sourceLineNo">273</span>     *<a name="line.273"></a>
