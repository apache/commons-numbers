<span class="sourceLineNo">248</span><a name="line.248"></a>
<span class="sourceLineNo">249</span>        for (int i = 0; i &lt; NUM_COEFF; i++) {<a name="line.249"></a>
<span class="sourceLineNo">250</span>            double result = 0;<a name="line.250"></a>
<span class="sourceLineNo">251</span>            final double[] row = AINV[i];<a name="line.251"></a>
