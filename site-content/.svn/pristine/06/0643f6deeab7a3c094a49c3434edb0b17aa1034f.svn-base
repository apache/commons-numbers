<span class="sourceLineNo">285</span><a name="line.285"></a>
<span class="sourceLineNo">286</span>    /**<a name="line.286"></a>
<span class="sourceLineNo">287</span>     * {@inheritDoc}<a name="line.287"></a>
<span class="sourceLineNo">288</span>     */<a name="line.288"></a>
<span class="sourceLineNo">289</span>    public double value(double x, double y) {<a name="line.289"></a>
<span class="sourceLineNo">290</span>        if (x &lt; 0 || x &gt; 1) {<a name="line.290"></a>
<span class="sourceLineNo">291</span>            throw new OutOfRangeException(x, 0, 1);<a name="line.291"></a>
<span class="sourceLineNo">292</span>        }<a name="line.292"></a>
<span class="sourceLineNo">293</span>        if (y &lt; 0 || y &gt; 1) {<a name="line.293"></a>
<span class="sourceLineNo">294</span>            throw new OutOfRangeException(y, 0, 1);<a name="line.294"></a>
<span class="sourceLineNo">295</span>        }<a name="line.295"></a>
<span class="sourceLineNo">296</span><a name="line.296"></a>
<span class="sourceLineNo">297</span>        final double x2 = x * x;<a name="line.297"></a>
<span class="sourceLineNo">298</span>        final double x3 = x2 * x;<a name="line.298"></a>
