<a class="jxr_linenumber" name="L423" href="#L423">423</a>     @Deprecated
<a class="jxr_linenumber" name="L424" href="#L424">424</a>     <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">static</strong> <strong class="jxr_keyword">void</strong> copy(<strong class="jxr_keyword">final</strong> <a href="../../../../../../../org/apache/commons/math3/stat/descriptive/rank/Percentile.html">Percentile</a> source, <strong class="jxr_keyword">final</strong> <a href="../../../../../../../org/apache/commons/math3/stat/descriptive/rank/Percentile.html">Percentile</a> dest)
<a class="jxr_linenumber" name="L425" href="#L425">425</a>         <strong class="jxr_keyword">throws</strong> <a href="../../../../../../../org/apache/commons/math3/exception/MathUnsupportedOperationException.html">MathUnsupportedOperationException</a> {
<a class="jxr_linenumber" name="L426" href="#L426">426</a>         <strong class="jxr_keyword">throw</strong> <strong class="jxr_keyword">new</strong> <a href="../../../../../../../org/apache/commons/math3/exception/MathUnsupportedOperationException.html">MathUnsupportedOperationException</a>();
