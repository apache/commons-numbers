<a class="jxr_linenumber" name="L947" href="#L947">947</a> <em class="jxr_javadoccomment">         * &amp;amp;minLimit = (5/8)/(N+1/4) &#92;&#92;</em>
<a class="jxr_linenumber" name="L948" href="#L948">948</a> <em class="jxr_javadoccomment">         * &amp;amp;maxLimit = (N-3/8)/(N+1/4) &#92;&#92;</em>
<a class="jxr_linenumber" name="L949" href="#L949">949</a> <em class="jxr_javadoccomment">         * \end{align}\)</em>
<a class="jxr_linenumber" name="L950" href="#L950">950</a> <em class="jxr_javadoccomment">         */</em>
<a class="jxr_linenumber" name="L951" href="#L951">951</a>         R_9(<span class="jxr_string">"R-9"</span>) {
<a class="jxr_linenumber" name="L952" href="#L952">952</a>             @Override
<a class="jxr_linenumber" name="L953" href="#L953">953</a>             <strong class="jxr_keyword">protected</strong> <strong class="jxr_keyword">double</strong> index(<strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">double</strong> p, <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">int</strong> length) {
