<a class="jxr_linenumber" name="L1010" href="#L1010">1010</a>             }
<a class="jxr_linenumber" name="L1011" href="#L1011">1011</a>             <strong class="jxr_keyword">if</strong> (pos &gt;= length) {
<a class="jxr_linenumber" name="L1012" href="#L1012">1012</a>                 <strong class="jxr_keyword">return</strong> selector.select(work, pivotsHeap, length - 1);
<a class="jxr_linenumber" name="L1013" href="#L1013">1013</a>             }
<a class="jxr_linenumber" name="L1014" href="#L1014">1014</a> 
<a class="jxr_linenumber" name="L1015" href="#L1015">1015</a>             <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">double</strong> lower = selector.select(work, pivotsHeap, intPos - 1);
