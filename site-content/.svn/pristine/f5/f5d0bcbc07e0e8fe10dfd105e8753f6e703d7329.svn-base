<a class="jxr_linenumber" name="L140" href="#L140">140</a>             <strong class="jxr_keyword">final</strong> SimpleRegression sub = <strong class="jxr_keyword">new</strong> SimpleRegression(includeIntercept);<em class="jxr_comment">// sub regression</em>
<a class="jxr_linenumber" name="L141" href="#L141">141</a>             <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">int</strong> i = 0; i &lt; 5; i++) { <em class="jxr_comment">// loop through individual samlpes.</em>
<a class="jxr_linenumber" name="L142" href="#L142">142</a>                 <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">double</strong> x = rand.nextDouble();
<a class="jxr_linenumber" name="L143" href="#L143">143</a>                 <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">double</strong> y = x * coef + rand.nextDouble();<em class="jxr_comment">// some noise</em>
<a class="jxr_linenumber" name="L144" href="#L144">144</a>                 sub.addData(x, y);
<a class="jxr_linenumber" name="L145" href="#L145">145</a>                 whole.addData(x, y);
<a class="jxr_linenumber" name="L146" href="#L146">146</a>             }
<a class="jxr_linenumber" name="L147" href="#L147">147</a>             parts.append(sub);
<a class="jxr_linenumber" name="L148" href="#L148">148</a>             Assert.assertTrue(equals(parts, whole, 1E-6));
