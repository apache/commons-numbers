<a class="jxr_linenumber" name="L132" href="#L132">132</a>     <strong class="jxr_keyword">private</strong> <strong class="jxr_keyword">void</strong> check(<strong class="jxr_keyword">boolean</strong> includeIntercept) {
<a class="jxr_linenumber" name="L133" href="#L133">133</a>         <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">int</strong> sets = 2;
<a class="jxr_linenumber" name="L134" href="#L134">134</a>         <strong class="jxr_keyword">final</strong> ISAACRandom rand = <strong class="jxr_keyword">new</strong> ISAACRandom(10L);<em class="jxr_comment">// Seed can be changed</em>
<a class="jxr_linenumber" name="L135" href="#L135">135</a>         <strong class="jxr_keyword">final</strong> SimpleRegression whole = <strong class="jxr_keyword">new</strong> SimpleRegression(includeIntercept);<em class="jxr_comment">// regression of the whole set</em>
<a class="jxr_linenumber" name="L136" href="#L136">136</a>         <strong class="jxr_keyword">final</strong> SimpleRegression parts = <strong class="jxr_keyword">new</strong> SimpleRegression(includeIntercept);<em class="jxr_comment">// regression with parts.</em>
<a class="jxr_linenumber" name="L137" href="#L137">137</a> 
<a class="jxr_linenumber" name="L138" href="#L138">138</a>         <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">int</strong> s = 0; s &lt; sets; s++) {<em class="jxr_comment">// loop through each subset of data.</em>
