<a class="jxr_linenumber" name="L641" href="#L641">641</a>     <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">void</strong> testRemoveMultiple() {
<a class="jxr_linenumber" name="L642" href="#L642">642</a>         <em class="jxr_comment">// Create regression with inference data then remove to test</em>
<a class="jxr_linenumber" name="L643" href="#L643">643</a>         SimpleRegression regression = <strong class="jxr_keyword">new</strong> SimpleRegression();
<a class="jxr_linenumber" name="L644" href="#L644">644</a>         regression.addData(infData);
<a class="jxr_linenumber" name="L645" href="#L645">645</a>         regression.removeData(removeMultiple);
<a class="jxr_linenumber" name="L646" href="#L646">646</a>         regression.addData(removeMultiple);
<a class="jxr_linenumber" name="L647" href="#L647">647</a>         <em class="jxr_comment">// Use the inference assertions to make sure that everything worked</em>
<a class="jxr_linenumber" name="L648" href="#L648">648</a>         Assert.assertEquals(<span class="jxr_string">"slope std err"</span>, 0.011448491,
