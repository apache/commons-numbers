<a class="jxr_linenumber" name="L650" href="#L650">650</a>         Assert.assertEquals(<span class="jxr_string">"std err intercept"</span>, 0.286036932,
<a class="jxr_linenumber" name="L651" href="#L651">651</a>                 regression.getInterceptStdErr(),1E-8);
<a class="jxr_linenumber" name="L652" href="#L652">652</a>         Assert.assertEquals(<span class="jxr_string">"significance"</span>, 4.596e-07,
<a class="jxr_linenumber" name="L653" href="#L653">653</a>                 regression.getSignificance(),1E-8);
<a class="jxr_linenumber" name="L654" href="#L654">654</a>         Assert.assertEquals(<span class="jxr_string">"slope conf interval half-width"</span>, 0.0270713794287,
<a class="jxr_linenumber" name="L655" href="#L655">655</a>                 regression.getSlopeConfidenceInterval(),1E-8);
<a class="jxr_linenumber" name="L656" href="#L656">656</a>      }
