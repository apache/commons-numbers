<a class="jxr_linenumber" name="L41" href="#L41">41</a>                  <strong class="jxr_keyword">return</strong> 2 * x[0] - 3 * x[1] + 5;
<a class="jxr_linenumber" name="L42" href="#L42">42</a>              }
<a class="jxr_linenumber" name="L43" href="#L43">43</a>          };
<a class="jxr_linenumber" name="L44" href="#L44">44</a>  
<a class="jxr_linenumber" name="L45" href="#L45">45</a>          <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">double</strong> darkFraction = 0.5;
<a class="jxr_linenumber" name="L46" href="#L46">46</a>          <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">double</strong> darkThreshold = 1e-2;
<a class="jxr_linenumber" name="L47" href="#L47">47</a>          <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">double</strong> background = Double.NaN;
<a class="jxr_linenumber" name="L48" href="#L48">48</a>          <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">double</strong> exponent = 1.1;
<a class="jxr_linenumber" name="L49" href="#L49">49</a>          <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">boolean</strong> shareSphere = <strong class="jxr_keyword">true</strong>;
<a class="jxr_linenumber" name="L50" href="#L50">50</a>          <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">double</strong> noInterpolationTolerance = Math.ulp(1d);
<a class="jxr_linenumber" name="L51" href="#L51">51</a>  
<a class="jxr_linenumber" name="L52" href="#L52">52</a>          <em class="jxr_comment">// N-dimensional interpolator.</em>
<a class="jxr_linenumber" name="L53" href="#L53">53</a>          <strong class="jxr_keyword">final</strong> MultivariateInterpolator interpolator
<a class="jxr_linenumber" name="L54" href="#L54">54</a>              = <strong class="jxr_keyword">new</strong> MicrosphereProjectionInterpolator(2, 500,
<a class="jxr_linenumber" name="L55" href="#L55">55</a>                                                      darkFraction,
<a class="jxr_linenumber" name="L56" href="#L56">56</a>                                                      darkThreshold,
<a class="jxr_linenumber" name="L57" href="#L57">57</a>                                                      background,
<a class="jxr_linenumber" name="L58" href="#L58">58</a>                                                      exponent,
<a class="jxr_linenumber" name="L59" href="#L59">59</a>                                                      shareSphere,
<a class="jxr_linenumber" name="L60" href="#L60">60</a>                                                      noInterpolationTolerance);
<a class="jxr_linenumber" name="L61" href="#L61">61</a>  
<a class="jxr_linenumber" name="L62" href="#L62">62</a>          <em class="jxr_comment">// 2D interpolator.</em>
<a class="jxr_linenumber" name="L63" href="#L63">63</a>          <strong class="jxr_keyword">final</strong> MultivariateInterpolator interpolator2D
<a class="jxr_linenumber" name="L64" href="#L64">64</a>              = <strong class="jxr_keyword">new</strong> MicrosphereProjectionInterpolator(<strong class="jxr_keyword">new</strong> InterpolatingMicrosphere2D(16,
<a class="jxr_linenumber" name="L65" href="#L65">65</a>                                                                                     darkFraction,
<a class="jxr_linenumber" name="L66" href="#L66">66</a>                                                                                     darkThreshold,
<a class="jxr_linenumber" name="L67" href="#L67">67</a>                                                                                     background),
<a class="jxr_linenumber" name="L68" href="#L68">68</a>                                                      exponent,
<a class="jxr_linenumber" name="L69" href="#L69">69</a>                                                      shareSphere,
<a class="jxr_linenumber" name="L70" href="#L70">70</a>                                                      noInterpolationTolerance);
<a class="jxr_linenumber" name="L71" href="#L71">71</a>  
<a class="jxr_linenumber" name="L72" href="#L72">72</a>          <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">double</strong> min = -1;
<a class="jxr_linenumber" name="L73" href="#L73">73</a>          <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">double</strong> max = 1;
<a class="jxr_linenumber" name="L74" href="#L74">74</a>          <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">double</strong> range = max - min;
<a class="jxr_linenumber" name="L75" href="#L75">75</a>          <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">int</strong> res = 5;
<a class="jxr_linenumber" name="L76" href="#L76">76</a>          <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">int</strong> n = res * res; <em class="jxr_comment">// Number of sample points.</em>
<a class="jxr_linenumber" name="L77" href="#L77">77</a>          <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">int</strong> dim = 2;
<a class="jxr_linenumber" name="L78" href="#L78">78</a>          <strong class="jxr_keyword">double</strong>[][] x = <strong class="jxr_keyword">new</strong> <strong class="jxr_keyword">double</strong>[n][dim];
<a class="jxr_linenumber" name="L79" href="#L79">79</a>          <strong class="jxr_keyword">double</strong>[] y = <strong class="jxr_keyword">new</strong> <strong class="jxr_keyword">double</strong>[n];
<a class="jxr_linenumber" name="L80" href="#L80">80</a>          <strong class="jxr_keyword">int</strong> index = 0;
<a class="jxr_linenumber" name="L81" href="#L81">81</a>          <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">int</strong> i = 0; i &lt; res; i++) {
<a class="jxr_linenumber" name="L82" href="#L82">82</a>              <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">double</strong> x1Val = toCoordinate(min, range, res, i);
<a class="jxr_linenumber" name="L83" href="#L83">83</a>              <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">int</strong> j = 0; j &lt; res; j++) {
<a class="jxr_linenumber" name="L84" href="#L84">84</a>                  <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">double</strong> x2Val = toCoordinate(min, range, res, j);
<a class="jxr_linenumber" name="L85" href="#L85">85</a>                  x[index][0] = x1Val;
<a class="jxr_linenumber" name="L86" href="#L86">86</a>                  x[index][1] = x2Val;
<a class="jxr_linenumber" name="L87" href="#L87">87</a>                  y[index] = f.value(x[index]);
<a class="jxr_linenumber" name="L88" href="#L88">88</a>                  ++index;
<a class="jxr_linenumber" name="L89" href="#L89">89</a>              }
<a class="jxr_linenumber" name="L90" href="#L90">90</a>          }
<a class="jxr_linenumber" name="L91" href="#L91">91</a>  
<a class="jxr_linenumber" name="L92" href="#L92">92</a>          <strong class="jxr_keyword">final</strong> MultivariateFunction p = interpolator.interpolate(x, y);
<a class="jxr_linenumber" name="L93" href="#L93">93</a>          <strong class="jxr_keyword">final</strong> MultivariateFunction p2D = interpolator2D.interpolate(x, y);
<a class="jxr_linenumber" name="L94" href="#L94">94</a>  
<a class="jxr_linenumber" name="L95" href="#L95">95</a>          <strong class="jxr_keyword">double</strong>[] c = <strong class="jxr_keyword">new</strong> <strong class="jxr_keyword">double</strong>[dim];
<a class="jxr_linenumber" name="L96" href="#L96">96</a>          <strong class="jxr_keyword">double</strong> expected, result, result2D;
<a class="jxr_linenumber" name="L97" href="#L97">97</a>  
<a class="jxr_linenumber" name="L98" href="#L98">98</a>          <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">int</strong> sampleIndex = 2;
<a class="jxr_linenumber" name="L99" href="#L99">99</a>          c[0] = x[sampleIndex][0];
<a class="jxr_linenumber" name="L100" href="#L100">100</a>         c[1] = x[sampleIndex][1];
<a class="jxr_linenumber" name="L101" href="#L101">101</a>         expected = f.value(c);
<a class="jxr_linenumber" name="L102" href="#L102">102</a>         result = p.value(c);
<a class="jxr_linenumber" name="L103" href="#L103">103</a>         result2D = p2D.value(c);
<a class="jxr_linenumber" name="L104" href="#L104">104</a>         Assert.assertEquals(<span class="jxr_string">"on sample point (exact)"</span>, expected, result2D, FastMath.ulp(1d));
<a class="jxr_linenumber" name="L105" href="#L105">105</a>         Assert.assertEquals(<span class="jxr_string">"on sample point (ND vs 2D)"</span>, result2D, result, FastMath.ulp(1d));
<a class="jxr_linenumber" name="L106" href="#L106">106</a> 
<a class="jxr_linenumber" name="L107" href="#L107">107</a>         <em class="jxr_comment">// Interpolation.</em>
<a class="jxr_linenumber" name="L108" href="#L108">108</a>         c[0] = 0.654321;
<a class="jxr_linenumber" name="L109" href="#L109">109</a>         c[1] = -0.345678;
<a class="jxr_linenumber" name="L110" href="#L110">110</a>         expected = f.value(c);
<a class="jxr_linenumber" name="L111" href="#L111">111</a>         result = p.value(c);
<a class="jxr_linenumber" name="L112" href="#L112">112</a>         result2D = p2D.value(c);
<a class="jxr_linenumber" name="L113" href="#L113">113</a>         Assert.assertEquals(<span class="jxr_string">"interpolation (exact)"</span>, expected, result2D, 1e-1);
<a class="jxr_linenumber" name="L114" href="#L114">114</a>         Assert.assertEquals(<span class="jxr_string">"interpolation (ND vs 2D)"</span>, result2D, result, 1e-1);
<a class="jxr_linenumber" name="L115" href="#L115">115</a> 
<a class="jxr_linenumber" name="L116" href="#L116">116</a>         <em class="jxr_comment">// Extrapolation.</em>
<a class="jxr_linenumber" name="L117" href="#L117">117</a>         c[0] = 0 - 1e-2;
<a class="jxr_linenumber" name="L118" href="#L118">118</a>         c[1] = 1 + 1e-2;
<a class="jxr_linenumber" name="L119" href="#L119">119</a>         expected = f.value(c);
<a class="jxr_linenumber" name="L120" href="#L120">120</a>         result = p.value(c);
<a class="jxr_linenumber" name="L121" href="#L121">121</a>         result2D = p2D.value(c);
<a class="jxr_linenumber" name="L122" href="#L122">122</a>         Assert.assertFalse(Double.isNaN(result));
<a class="jxr_linenumber" name="L123" href="#L123">123</a>         Assert.assertFalse(Double.isNaN(result2D));
<a class="jxr_linenumber" name="L124" href="#L124">124</a>         Assert.assertEquals(<span class="jxr_string">"extrapolation (exact)"</span>, expected, result2D, 1e-1);
<a class="jxr_linenumber" name="L125" href="#L125">125</a>         Assert.assertEquals(<span class="jxr_string">"extrapolation (ND vs 2D)"</span>, result2D, result, 1e-2);
<a class="jxr_linenumber" name="L126" href="#L126">126</a> 
<a class="jxr_linenumber" name="L127" href="#L127">127</a>         <em class="jxr_comment">// Far away.</em>
<a class="jxr_linenumber" name="L128" href="#L128">128</a>         c[0] = 20;
<a class="jxr_linenumber" name="L129" href="#L129">129</a>         c[1] = -30;
<a class="jxr_linenumber" name="L130" href="#L130">130</a>         result = p.value(c);
<a class="jxr_linenumber" name="L131" href="#L131">131</a>         Assert.assertTrue(result + <span class="jxr_string">" should be NaN"</span>, Double.isNaN(result));
<a class="jxr_linenumber" name="L132" href="#L132">132</a>         result2D = p2D.value(c);
<a class="jxr_linenumber" name="L133" href="#L133">133</a>         Assert.assertTrue(result2D + <span class="jxr_string">" should be NaN"</span>, Double.isNaN(result2D));
<a class="jxr_linenumber" name="L134" href="#L134">134</a>     }
