<a class="jxr_linenumber" name="L762" href="#L762">762</a>     <em class="jxr_javadoccomment">/**</em>
<a class="jxr_linenumber" name="L763" href="#L763">763</a> <em class="jxr_javadoccomment">     * {@inheritDoc}</em>
<a class="jxr_linenumber" name="L764" href="#L764">764</a> <em class="jxr_javadoccomment">     * @since 3.1</em>
<a class="jxr_linenumber" name="L765" href="#L765">765</a> <em class="jxr_javadoccomment">     */</em>
<a class="jxr_linenumber" name="L766" href="#L766">766</a>     <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">boolean</strong> isSupportConnected() {
<a class="jxr_linenumber" name="L767" href="#L767">767</a>         <strong class="jxr_keyword">return</strong> <strong class="jxr_keyword">true</strong>;
