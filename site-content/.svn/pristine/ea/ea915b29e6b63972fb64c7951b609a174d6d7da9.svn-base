<span class="sourceLineNo">036</span>public class HarmonicOscillator implements UnivariateDifferentiableFunction, DifferentiableUnivariateFunction {<a name="line.36"></a>
<span class="sourceLineNo">037</span>    /** Amplitude. */<a name="line.37"></a>
<span class="sourceLineNo">038</span>    private final double amplitude;<a name="line.38"></a>
<span class="sourceLineNo">039</span>    /** Angular frequency. */<a name="line.39"></a>
