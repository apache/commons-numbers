<span class="sourceLineNo">056</span>        this.phase = phase;<a name="line.56"></a>
<span class="sourceLineNo">057</span>    }<a name="line.57"></a>
<span class="sourceLineNo">058</span><a name="line.58"></a>
<span class="sourceLineNo">059</span>    /** {@inheritDoc} */<a name="line.59"></a>
<span class="sourceLineNo">060</span>    public double value(double x) {<a name="line.60"></a>
<span class="sourceLineNo">061</span>        return value(omega * x + phase, amplitude);<a name="line.61"></a>
<span class="sourceLineNo">062</span>    }<a name="line.62"></a>
<span class="sourceLineNo">063</span><a name="line.63"></a>
<span class="sourceLineNo">064</span>    /** {@inheritDoc}<a name="line.64"></a>
